                  <xs:enumeration value="InteriorBlind"/>
                  <xs:enumeration value="ExteriorBlind"/>
                  <xs:enumeration value="BetweenGlassBlind"/>
                  <xs:enumeration value="SwitchableGlazing"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="ControlType">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="AlwaysOn"/>
                  <xs:enumeration value="AlwaysOff"/>
                  <xs:enumeration value="OnIfScheduleAllows"/>
                  <xs:enumeration value="OnIfHighSolarOnWindow"/>
                  <xs:enumeration value="OnIfHighHorizontalSolar"/>
                  <xs:enumeration value="OnIfHighOutdoorAirTemperature"/>
                  <xs:enumeration value="OnIfHighZoneAirTemperature"/>
                  <xs:enumeration value="OnIfHighZoneCooling"/>
                  <xs:enumeration value="OnIfHighGlare"/>
                  <xs:enumeration value="MeetDaylightIlluminanceSetpoint"/>
                  <xs:enumeration value="OnNightIfLowOutdoorTempAndOffDay"/>
                  <xs:enumeration value="OnNightIfLowInsideTempAndOffDay"/>
                  <xs:enumeration value="OnNightIfHeatingAndOffDay"/>
                  <xs:enumeration value="OnNightIfLowOutdoorTempAndOnDayIfCooling"/>
                  <xs:enumeration value="OnNightIfHeatingAndOnDayIfCooling"/>
                  <xs:enumeration value="OffNightAndOnDayIfCoolingAndHighSolarOnWindow"/>
                  <xs:enumeration value="OnNightAndOnDayIfCoolingAndHighSolarOnWindow"/>
                  <xs:enumeration value="OnIfHighOutdoorAirTempAndHighSolarOnWindow"/>
                  <xs:enumeration value="OnIfHighOutdoorAirTempAndHighHorizontalSolar"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="Setpoint" type="xs:double" minOccurs="0">
              <xs:annotation>
                <xs:documentation>{W/m2} for solar based controls, {W} for cooling or heating based controls, and {C} for temperature based controls.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ShadingIsScheduled" type="xs:boolean" default="false"/>
            <xs:element name="ScheduleName" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Required if control type is scheduled = yes. Must match valid schedule.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="GlareControl" type="xs:boolean" default="false"/>
            <xs:element name="MaterialName"/>
            <xs:element name="SlatAngleControl">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="FixedSlatAngle"/>
                  <xs:enumeration value="ScheduledSlatAngle"/>
                  <xs:enumeration value="BlockBeamSolar"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="SlatAngleSchedule" type="xs:string" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="WindowShade" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The window shading inputs mirrors the EnergyPlus Window Shading inputs.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:attribute name="MaterialName" type="xs:string" use="required"/>
          <xs:attribute name="SolarTrans" type="xs:double" use="required"/>
          <xs:attribute name="SolarReflect" type="xs:double" use="required"/>
          <xs:attribute name="VisibleTrans" type="xs:double" use="required"/>
          <xs:attribute name="VisibleReflect" type="xs:double" use="required"/>
          <xs:attribute name="IREmm" type="xs:double" use="required"/>
          <xs:attribute name="IRTrans" type="xs:double" use="required"/>
          <xs:attribute name="Thickness" type="xs:double" use="required"/>
          <xs:attribute name="Conductivity" type="xs:double" use="required"/>
          <xs:attribute name="ShadeToGlassDistance" type="xs:double" use="required"/>
          <xs:attribute name="TopOpenMult" type="xs:double" use="required"/>
          <xs:attribute name="BottomOpenMult" type="xs:double" use="required"/>
          <xs:attribute name="LeftOpenMult" type="xs:double" use="required"/>
          <xs:attribute name="RightOpenMult" type="xs:double" use="required"/>
          <xs:attribute name="AirPermeability" type="xs:double" use="required"/>
        </xs:complexType>
      </xs:element>
      <xs:choice minOccurs="0">
        <xs:element name="SimpleGlazingSystem" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Simple window model.  Cannot be used in conjunction with WindowGlass or WindowGas, it replaces those.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Name" type="xs:string"/>
              <xs:element name="UValue" type="xs:float"/>
              <xs:element name="SHGC" type="xs:float"/>
              <xs:element name="VT" type="xs:float" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:choice minOccurs="0" maxOccurs="10">
          <xs:element name="WindowGlass">
            <xs:annotation>
              <xs:documentation>The window glass inputs mirrors the EnergyPlus Window Glass inputs.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attribute name="MaterialName" type="xs:string" use="required">
                <xs:annotation>
                  <xs:documentation>Name of material.  If the name is the same as other material, then it will use the properties of the first instance of the material name.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="Thickness" type="xs:double" use="required"/>
              <xs:attribute name="SolarTrans" type="xs:double" use="required"/>
              <xs:attribute name="SolarReflectFront" type="xs:double"/>
              <xs:attribute name="SolarReflectBack" type="xs:double"/>
              <xs:attribute name="VisibleTrans" type="xs:double"/>
              <xs:attribute name="VisibleReflectFront" type="xs:double"/>
              <xs:attribute name="VisibleReflectBack" type="xs:double"/>
              <xs:attribute name="IRTrans" type="xs:double"/>
              <xs:attribute name="IREmmFront" type="xs:double"/>
              <xs:attribute name="IREmmBack" type="xs:double"/>
              <xs:attribute name="Conductivity" type="xs:double"/>
              <xs:attribute name="Comments" type="xs:string"/>
            </xs:complexType>
          </xs:element>
          <xs:element name="WindowGas">
            <xs:annotation>
              <xs:documentation>The window gas inputs mirrors the EnergyPlus Window Gas inputs.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attribute name="MaterialName" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Name of material.  If the name is the same as other material, then it will use the properties of the first instance of the material name.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="GasType">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="Air"/>
                    <xs:enumeration value="Krypton"/>
                    <xs:enumeration value="Argon"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:attribute>
              <xs:attribute name="Thickness" type="xs:double"/>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OpaqueConstruction">
    <xs:sequence>
      <xs:element name="ConstructionName">
        <xs:annotation>
          <xs:documentation>Name of construction.  Must be unique.  If the same name as other construction, then it will use the first instance of the construction name.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:attribute name="instance" type="xs:string"/>
          <xs:attribute name="description" type="xs:string">
            <xs:annotation>
              <xs:documentation>Detailed description of the construction.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attributeGroup ref="CostDist"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="Performance" minOccurs="0">
        <xs:annotation>
          <xs:documentation>This is the perfomance of the construction that is passed through the preprocessor.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:all>
            <xs:element name="Type" type="xs:string" minOccurs="0"/>
            <xs:element name="UValue" type="xs:float" minOccurs="0"/>
          </xs:all>
        </xs:complexType>
      </xs:element>
      <xs:element name="HighAlbedo" type="xs:boolean" minOccurs="0"/>
      <xs:element name="InfiltrationPerArea" type="xs:double" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Amount of infiltration this object attributes to the attached zone {m3/s/m2}.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LowTemperatureRadiantProperties" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="SourcePresentAfterLayerNumber" type="xs:integer">
              <xs:annotation>
                <xs:documentation>This field is an integer that relates the location of the heat source or sink. The integer refers to the list of material layers that follow later in the syntax and determines the layer after which the source is present. If a source is embedded within a single homogenous layer (such as concrete), that layer should be split into two layers and the source added between them. For example, a value of “2” in this field tells EnergyPlus that the source is located between the second and third material layers listed later in the construction description.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="TempCalcAfterLayerNumber" type="xs:integer">
              <xs:annotation>
                <xs:documentation>The nature of this field is similar to the source interface parameter (see previous field) in that it is an integer, refers to the list of material layers that follow, and defines a location after the layer number identified by the user-defined number. In this case, the user is specifying the location for a separate temperature calculation rather than the location of the heat source/sink. This feature is intended to allow users to calculate a temperature within the construction. This might be important in a radiant cooling system where condensation could be a problem. This temperature calculation can assist users in making that determination in absence of a full heat and mass balance calculation.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DimensionsForCTFCalculation" type="xs:integer">
              <xs:annotation>
                <xs:documentation>This field is also an integer and refers to the detail level of the calculation. A value of “1” states that the user is only interested in a one-dimensional calculation. This is appropriate for electric resistance heating and for hydronic heating (when boiler/hot water heater performance is not affected by return and supply water temperatures). A value of “2” will trigger a two-dimensional solution for this surface only. This may be necessary for hydronic radiant cooling situations since chiller performance is affected by the water temperatures provided.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="TubeSpacing" type="xs:double">
              <xs:annotation>
                <xs:documentation>This field defines how far apart in meters the hydronic tubing or electrical resistance wires are spaced in the direction perpendicular to the main direction of heat transfer. Note that this parameter is only used for two-dimensional solutions (see previous field).</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:choice minOccurs="0" maxOccurs="10">
        <xs:element name="Material">
          <xs:annotation>
            <xs:documentation>The material input mirros the EnergyPlus Material input.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:attribute name="MaterialName" type="xs:string" use="required">
              <xs:annotation>
                <xs:documentation>Name of material.  If the name is the same as other material, then it will use the properties of the first instance of the material name.</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Roughness">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="Smooth"/>
                  <xs:enumeration value="MediumSmooth"/>
                  <xs:enumeration value="MediumRough"/>
                  <xs:enumeration value="Rough"/>
                  <xs:enumeration value="VeryRough"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Thickness" type="xs:double"/>
            <xs:attribute name="Conductivity" type="xs:double"/>
            <xs:attribute name="Density" type="xs:double"/>
            <xs:attribute name="SpecificHeat" type="xs:double"/>
            <xs:attribute name="AbsorThermal" type="xs:double"/>
            <xs:attribute name="AbsorSolar" type="xs:double"/>
            <xs:attribute name="AbsorVis" type="xs:double"/>
            <xs:attribute name="Comments" type="xs:string"/>
          </xs:complexType>
        </xs:element>
        <xs:element name="Material-R">
          <xs:annotation>
            <xs:documentation>The material-r input mirrors the EnergyPlus Material-R input.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:attribute name="MaterialName" type="xs:string"/>
            <xs:attribute name="Roughness">
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="Smooth"/>
                  <xs:enumeration value="MediumSmooth"/>
                  <xs:enumeration value="MediumRough"/>
                  <xs:enumeration value="Rough"/>
                  <xs:enumeration value="VeryRough"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="ThermalResistance" type="xs:double"/>
            <xs:attribute name="AbsorThermal" type="xs:double"/>
            <xs:attribute name="AbsorSolar" type="xs:double"/>
            <xs:attribute name="AbsorVis" type="xs:double"/>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FenestrationApply">
    <xs:annotation>
      <xs:documentation>Settings for applying the fenestration.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ApplyType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Fraction of exterior wall area that are windows.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="BottomUp"/>
            <xs:enumeration value="TopDown"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AreaFraction" default="0.4" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Fraction of exterior wall area that are windows.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:double">
              <xs:attributeGroup ref="CostDist"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="SillHeight" default="1.1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Height offset for windows {m}.  If area available is less than requested area, then the sill height will be reduced in an attempt to fit the glazing.  This only applies if ApplyType is bottomup</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:double">
              <xs:attributeGroup ref="CostDist"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="HeaderHeight" default="0.5" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Header height offset for windows {m}.  If area available is less than requested area, then the header height will be reduced in an attempt to fit the glazing. This only applies if ApplyType is topdown.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:double">
              <xs:attributeGroup ref="CostDist"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="EdgeOffset" default="0.05" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Offsets for the edges of the window {m}.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:double">
              <xs:attributeGroup ref="CostDist"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Only Banded glazing works currently.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Banded"/>
            <xs:enumeration value="Discrete"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Construction" type="WindowConstruction" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Custom construction for this window.  Overrides the standard construction.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Overhang" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Window overhang.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:choice>
              <xs:element name="Depth">
                <xs:annotation>
                  <xs:documentation>Depth of window overhang {m}.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="ProjectionFactor">
                <xs:annotation>
                  <xs:documentation>Fraction of window height that is projected out as overhang.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:element name="Offset">
              <xs:annotation>
                <xs:documentation>Offset above window where overhang starts {m}.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:double">
                    <xs:attributeGroup ref="CostDist"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Fin" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Window fins.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:choice>
              <xs:element name="Depth">
                <xs:annotation>
                  <xs:documentation>Depth of window fin {m}.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="ProjectionFactor">
                <xs:annotation>
                  <xs:documentation>Fraction of window width that is projected out as fin.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:element name="Spacing" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Spacing between the fins {m}.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:double">
                    <xs:attributeGroup ref="CostDist"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="Offset">
              <xs:annotation>
                <xs:documentation>Offset to the sides of the window where the fins start {m}.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:double">
                    <xs:attributeGroup ref="CostDist"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Gains">
    <xs:sequence>
      <xs:element name="People" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:sequence>
              <xs:element name="PeopleDensity" type="xs:double" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>People density {People/100 m2}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="TotalPeople" type="xs:double" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Total number of people in space.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
            <xs:element ref="Schedule" minOccurs="0"/>
          </xs:sequence>
          <xs:attributeGroup ref="CostDist"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="Lights" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Design Watts per m^2, cost is per kW of Design Level</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:sequence>
              <xs:element name="PowerDensity" type="xs:double" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Lighting power density {W/m2}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="TotalPower" minOccurs="0"/>
              <xs:element name="PowerPerPerson" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Lighting power per person (W/person)</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
            <xs:element name="FractionRadiant" type="xs:double" default="0.37" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Recessed Lighting 0.37; Suspended 0.42;</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="FractionReturn" type="xs:double" default="0.2" minOccurs="0"/>
            <xs:choice>
              <xs:element ref="Schedule" minOccurs="0"/>
              <xs:element name="HoursPerDay" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Use this to automatically create a schedule for the number of hours.  The schedule is simple and starts at hour 1.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
            <xs:sequence>
              <xs:element name="SchFileColumn" type="xs:integer" minOccurs="0"/>
              <xs:element name="SchFileRowSkip" type="xs:integer" minOccurs="0"/>
            </xs:sequence>
            <xs:element name="EndUseCategory" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Category where lighting is reported in EnergyPlus result file.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="CostDist"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="Equipment" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Equipment loads.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="EquipmentName" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Name of equipment.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="EquipmentType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Type of equipment {Electric, Gas}.</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="Electric"/>
                  <xs:enumeration value="Gas"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:choice>
              <xs:element name="PowerDensity" type="xs:double">
                <xs:annotation>
                  <xs:documentation>Equipment power density {W/m2}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="TotalPower" type="xs:double">
                <xs:annotation>
                  <xs:documentation>Total equipment power {W}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PowerPerPerson" type="xs:double">
                <xs:annotation>
                  <xs:documentation>Equipment power per person (W/person)</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
            <xs:element name="RadiantFraction" type="xs:double" default="0.5" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Equipment radiant fraction.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="LatentFraction" type="xs:double" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Optional: Equipment latent fraction.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="LostFraction" type="xs:double" default="0.5" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Optional: Fraction of gain that is lost from zone</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="Schedule" minOccurs="0"/>
            <xs:sequence>
              <xs:element name="SchFileColumn" type="xs:integer" minOccurs="0"/>
              <xs:element name="SchFileRowSkip" type="xs:integer" minOccurs="0"/>
            </xs:sequence>
            <xs:element name="EndUseCategory" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Category where equipment is reported in EnergyPlus result file.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="CostDist"/>
        </xs:complexType>
      </xs:element>
