                                    <xs:annotation>
                                      <xs:documentation>Optional, defaults to 10 C.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="MaxSupplyAirTemp" default="xs:double" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Optional, defaults to 50 C.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="SingleDuctConstantFlow" minOccurs="0">
                              <xs:complexType/>
                            </xs:element>
                            <xs:element name="SingleDuctVariableFlow" minOccurs="0">
