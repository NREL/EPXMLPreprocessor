                                    </xs:annotation>
                                    <xs:simpleType>
                                      <xs:restriction base="xs:string">
                                        <xs:enumeration value="Off"/>
                                        <xs:enumeration value="SimpleOff"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:element>
                                  <xs:element name="CondensationControlDewpointOffset" type="xs:float" minOccurs="0"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:choice minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Terminal units</xs:documentation>
                            </xs:annotation>
                            <xs:element name="DirectAir" minOccurs="0">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="AirSystemName" type="xs:string"/>
                                  <xs:element name="AirFlowRate" type="xs:float" minOccurs="0"/>
