                      </xs:simpleType>
                    </xs:element>
                    <xs:choice>
                      <xs:element name="PVAreaFraction" default="0">
                        <xs:annotation>
                          <xs:documentation>Fraction of surface the has PV.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:double">
                            <xs:minInclusive value="0"/>
                            <xs:maxInclusive value="1"/>
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:element>
                      <xs:element name="PVInstalledCap" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>Total installed capacity of PV {kW}.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:choice>
                    <xs:element name="PVEfficiency" type="xs:double" default="0.10">
                      <xs:annotation>
                        <xs:documentation>PV cell efficiency fraction.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="PVInverterEff" type="xs:double" default="0.91">
                      <xs:annotation>
                        <xs:documentation>PV inverter efficiency fraction.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="IntegrationMode" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>PV integration.  Currently only decoupled works.</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="Decoupled"/>
                          <xs:enumeration value="Integrated"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="OrientationAngle" type="xs:double" default="0" minOccurs="0" maxOccurs="360">
                      <xs:annotation>
                        <xs:documentation>Orientation of outward PV normal in degrees (North facing = 0, East facing =90, South facing = 180, West facing = 270)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="TiltAngle" type="xs:double" default="0" minOccurs="0" maxOccurs="90">
                      <xs:annotation>
                        <xs:documentation>Angle of PV tilt from horizontal in degrees</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ExistingSurfaces" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>Apply PV directly to a list of existing surfaces. PV must be sized by area fraction when using this mode. </xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="SurfaceName" type="xs:string"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attributeGroup ref="CostDist"/>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
