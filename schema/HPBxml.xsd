                            <xs:element name="BaseboardHeaterWater" minOccurs="0"/>
                            <xs:element name="HighTemperatureRadiant" minOccurs="0"/>
                            <xs:element name="LowTempRadiantVariableFlow" minOccurs="0">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="Name" type="xs:string" minOccurs="0"/>
                                  <xs:element name="LiquidSystemHeatingName" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Name of the hydronic heating system.  Enter "NONE" if there is no heating.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="LiquidSystemCoolingName" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Name of the hydronic cooling system.  Enter "NONE" if there is no cooling.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="Surfaces" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>List of surfaces in the zone that this system applies to.  If this is blank then the system will apply to all low temperature radiant surfaces in the zone.  </xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element name="Surface" minOccurs="0" maxOccurs="unbounded"/>
                                      </xs:sequence>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element name="AvailabilitySchedule" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Availability schedule name.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="TubingInsideDiameter" type="xs:double" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Hydronic tubing diamter {m}. Must be specified.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="TubingLength" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Total length of pipe embedded in surface {m}. Will autosize unless a value is specified.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="TempControlType" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Radiant system temperature control type.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                      <xs:restriction base="xs:string">
                                        <xs:enumeration value="MeanAirTemperature"/>
                                        <xs:enumeration value="MeanRadiantTemperature"/>
                                        <xs:enumeration value="OperativeTemperature"/>
                                        <xs:enumeration value="OutdoorDryBulbTemperature"/>
                                        <xs:enumeration value="OutdoorWetBuilbTemperature"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:element>
                                  <xs:element name="MaxHotWaterFlow" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Maximum hot water flow rate {m3/s}. Will autosize unless a value is specified.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="HeatingThrottlingRange" type="xs:double" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Temperature range over which the radiant system throttles from zero flow rate up to the maximum hot water flow rate field {deg C}.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="HeatingTempSchedule" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Temperature schedule name. Will default unless specified.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="MaxChilledWaterFlow" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Maximum chilled water flow rate {m3/s}. Will autosize unless a value is specified.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="CoolingThrottlingRange" type="xs:double" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Temperature range over which the radiant system throttles from zero flow rate up to the maximum chilled water flow rate field {deg C}.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="CoolingTempSchedule" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Temperature schedule name. Will default unless specified.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="CondensationControlType" minOccurs="0">
