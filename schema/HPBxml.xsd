  <xs:element name="HPBParametization">
    <xs:annotation>
      <xs:documentation>Main element</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="UserInfo" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Contains specific user information</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="EmailAddress" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="HPBXmlSettings">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="HPBXmlVersion">
                <xs:annotation>
                  <xs:documentation>Current HPBXml schema version, mirrors that in Preprocessor up to build number</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="0.1.2.*"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="OptEPlusMetadata" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="OptEPlusVersion" type="xs:string" minOccurs="0"/>
              <xs:element name="OptEPlusDatabase" type="xs:string" minOccurs="0"/>
              <xs:element name="InputIDs" type="xs:string" minOccurs="0"/>
              <xs:element name="InputTypeInstances" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="WebInterfaceSettings" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Version" type="xs:string" minOccurs="0"/>
              <xs:element name="InputIDs" type="xs:string" minOccurs="0"/>
              <xs:element name="EmailUpdateMessage" type="xs:string" minOccurs="0"/>
              <xs:element name="BuildingDescription" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SketchUpSettings" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Version" type="xs:double" minOccurs="0"/>
              <xs:element name="ServerDir" type="xs:string" minOccurs="0"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="SimSettings">
          <xs:annotation>
            <xs:documentation>Contains information to control how the simulation is ran</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="TimeStepInHour" type="xs:int" default="4" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Simulation time steps in hour (defaults to 4).</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="TimeStepsInAveragingWindow" type="xs:int" default="4" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Number of load time steps in the zone design flow sequence averaging window. If this field is omitted, the value will be equal to the time step in hour for time steps of 1, 2, 3, 4, 6, 12, 15, 30, 60; otherwise the value will be 3.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RunStartMonth" type="xs:int" default="1" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Simulation start month. (Defaults to 1)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RunStartDay" type="xs:int" default="1" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Simulation start day.  (Defaults to 1)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RunStopMonth" type="xs:int" default="12" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Simulation stop month. (Defaults to 12)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RunStopDay" type="xs:int" default="31" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Simulation stop day.  (Defaults to 31)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Holidays" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Optional, defaults to true. False will not put holidays in the IDF.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="MaxNumWarmupDays" type="xs:int" default="25" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Number of simulation warm up days. (Defaults to 25)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="LoadConvergeTolerance" type="xs:double" default="0.04" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Load convergence tolerance.  (Defaults to 0.04)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="TempConvergeTolerance" type="xs:double" default="0.2" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Temperature convergence tolerance.  (Defaults to 0.2)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="UpdateShadowInterval" type="xs:int" default="30" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Shadowing update interval. (Defaults to 25)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="BaseCase" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Basecase flag.  If set to true, OptEPlus will perform a different set of calculations. (Defaults to false)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="BenchmarkHeader" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>DOE benchmark flag.  If set to true then the IDF file will have a header with comments for the DOE benchmarks.  (Defaults to false)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="RunWeatherFile" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Run weather file flag.  If set to true then the weather file is used, otherwise, the preprocessor will only create the input file and run the design day.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DoPlantSizingCalc" type="xs:boolean" default="true" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Plant sizing calculation flag used by EnergyPlus. The default is true unless the user specifies false.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="SQLiteOutput" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Defaults to false.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Costs" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Defaults to true. If false, no cost data will be written to the IDF.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="MaxHVACIterations" type="xs:int" default="5" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Maximum HVAC iterations.  (Defaults to 5)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="MinSystemTimeStep" type="xs:int" default="5" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Minimum system time step.  (Defaults to 5)</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="EnergyPlusVersion" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Developer special. Controls version of EnergyPlus. Used to flag transitioning changed input objects in future versions of EnergyPlus.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="8.0"/>
                    <xs:enumeration value="8.1"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="SizingFactor" type="xs:float" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Defaults to 1.2</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
