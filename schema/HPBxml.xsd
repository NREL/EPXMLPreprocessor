                                      <xs:enumeration value="Unknown"/>
                                      <xs:enumeration value="SunExposed"/>
                                      <xs:enumeration value="NoSun"/>
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="WindExposure" default="Unknown">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:enumeration value="Unknown"/>
                                      <xs:enumeration value="WindExposed"/>
                                      <xs:enumeration value="NoWind"/>
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="Vertices" minOccurs="3" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:attribute name="X1" type="xs:double" default="1"/>
                                    <xs:attribute name="Y1" type="xs:double" default="1"/>
                                    <xs:attribute name="Z1" type="xs:double" default="0"/>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="CustomXYZSubSurfaces" minOccurs="0" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="SubSurfaceName" type="xs:string"/>
                                      <xs:element name="SubSurfaceType">
                                        <xs:annotation>
                                          <xs:documentation>Generic sub-surface type

WARNING: maps to IDF

ISSUES: dome and diffuser for tdd should move to specific type</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:string">
                                            <xs:enumeration value="Window"/>
                                            <xs:enumeration value="DaylightWindow"/>
                                            <xs:enumeration value="Skylight"/>
                                            <xs:enumeration value="Door"/>
                                            <xs:enumeration value="GlassDoor"/>
                                            <xs:enumeration value="TDD:Dome"/>
                                            <xs:enumeration value="TDD:Diffuser"/>
                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="SubSurfaceSpecificType" default="Unknown">
                                        <xs:annotation>
                                          <xs:documentation>Specific sub-surface type

ISSUES: possible to combine non-sensical items, e.g. door with non-operable</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:string">
                                            <xs:enumeration value="Unknown"/>
                                            <xs:enumeration value="Operable"/>
                                            <xs:enumeration value="Non-Operable"/>
                                            <xs:enumeration value="Swinging"/>
                                            <xs:enumeration value="Non-Swinging"/>
                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="ConstructionName" type="xs:string" default="Default"/>
                                      <xs:element name="OutsideFaceEnvObject" type="xs:string">
                                        <xs:annotation>
                                          <xs:documentation>If the subsurface type is TDD:Dome, put the name of the corresponding diffuser here for custom TDDs.</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                      <xs:element name="Multiplier" type="xs:double"/>
                                      <xs:element name="SubVertices" minOccurs="3" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:attribute name="X1" type="xs:double" default="1"/>
                                          <xs:attribute name="Y1" type="xs:double" default="1"/>
                                          <xs:attribute name="Z1" type="xs:double" default="0"/>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="CustomXYZShading" minOccurs="0" maxOccurs="unbounded">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="ShadingName" type="xs:string"/>
                                      <xs:element name="ShadingTransmittanceSchedule" type="xs:string" default="ALWAYS_OFF"/>
                                      <xs:element name="ShadingVertices" minOccurs="3" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:attribute name="X1" type="xs:double" default="1"/>
                                          <xs:attribute name="Y1" type="xs:double" default="1"/>
                                          <xs:attribute name="Z1" type="xs:double" default="0"/>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Ceiling" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Ceiling configuration.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Adiabatic" type="xs:boolean" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Sets the ceiling surface to adiabatic.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="EnableTopLight" type="xs:boolean" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Intended for applying EDM measures with optimization platform.  If true, it will consider top light with skylights or TDDs.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="EnableTopLightMonitors" type="xs:boolean" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Intended for applying EDM measures with optimization platform.  If true, it will consider top light with monitors.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Construction" type="OpaqueConstruction" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Custom construction for this ceiling surface.  Overrides the standard construction.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:choice>
                            <xs:element name="TubularDaylightingDevices" minOccurs="0">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="InstallConfiguration">
                                    <xs:annotation>
                                      <xs:documentation>Stringkey detailing how the TDDs are installed.  This only applies when using the "ALL" zone name.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                      <xs:restriction base="xs:string">
                                        <xs:enumeration value="All Top Zones"/>
                                        <xs:enumeration value="All Top Zones Without Windows"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:element>
                                  <xs:element name="AreaPerTDD" type="xs:double">
                                    <xs:annotation>
                                      <xs:documentation>Area per TDD {m2}.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                </xs:sequence>
                                <xs:attributeGroup ref="CostDist"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="Skylights" minOccurs="0">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="InstallConfiguration">
                                    <xs:annotation>
                                      <xs:documentation>Stringkey detailing how the TDDs are installed.  This only applies when using the "ALL" zone name.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                      <xs:restriction base="xs:string">
                                        <xs:enumeration value="All Top Zones"/>
                                        <xs:enumeration value="All Top Zones Without Windows"/>
                                      </xs:restriction>
                                    </xs:simpleType>
                                  </xs:element>
                                  <xs:element name="AreaFraction">
                                    <xs:annotation>
                                      <xs:documentation>Fraction of ceiling surface with skylights.  Only applies to ceilings exposed to exterior environment.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                      <xs:simpleContent>
                                        <xs:extension base="xs:double"/>
                                      </xs:simpleContent>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element name="Construction" type="OpaqueConstruction" minOccurs="0"/>
                                </xs:sequence>
                                <xs:attributeGroup ref="CostDist"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Floor" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Floor configuration.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Adiabatic" type="xs:boolean" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Sets the floor surface to adiabatic.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Construction" type="OpaqueConstruction" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Custom construction for this floor surface.  Overrides the standard construction.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Surfaces" minOccurs="0" maxOccurs="unbounded">
                      <xs:annotation>
                        <xs:documentation>Vertical surface configuration.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="SurfaceNormal">
                            <xs:annotation>
                              <xs:documentation>If a direction, then it applies to all surfaces facing that direction.  Otherwise it will affect the surface number based on the the index</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:enumeration value="All"/>
                                <xs:enumeration value="South"/>
                                <xs:enumeration value="East"/>
                                <xs:enumeration value="North"/>
                                <xs:enumeration value="West"/>
                                <xs:enumeration value="Name"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="SurfaceName" type="xs:string" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Only needed if SurfaceNormal is "name", then has to be the name of the surface to manipulate</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ExteriorWalls" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Surface configuration for exterior wall.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Adiabatic" type="xs:boolean" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Sets the floor surface to adiabatic.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="Construction" type="OpaqueConstruction" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Custom construction for this exterior wall.  Overrides the standard construction.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="InteriorWalls" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Surface configuration for the interior wall.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Construction" type="OpaqueConstruction" minOccurs="0">
                                  <xs:annotation>
                                    <xs:documentation>Custom construction for this exterior wall.  Overrides the standard construction.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Fenestration" type="FenestrationApply" minOccurs="0"/>
                          <xs:element name="DaylightFenestration" type="FenestrationApply" minOccurs="0"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="InternalGains" type="Gains" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Internal gains configurations.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="InternalMassMultiplier" type="xs:double" default="1" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Multiplies default value of 162 kg/m2 of floor area of internal mass (furniture) in each zone. Note: setting this >1 may cause the total internal mass area to exceed the total opaque surface area of the zone and results in EnergyPlus warnings or errors.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="ExternalGains" type="Gains" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Gains not added to the zone (as exterior load)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Daylighting" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Zone daylighting.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ControlType" default="None">
                            <xs:annotation>
                              <xs:documentation>Stringkey of daylighting control type.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:enumeration value="Continuous"/>
                                <xs:enumeration value="Stepped"/>
                                <xs:enumeration value="Continuous/off"/>
                                <xs:enumeration value="None"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="Setpoint" type="xs:double" default="0" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Daylighting setpoint {lux}.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="NumberOfSteps" type="xs:int" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Number of steps, if ControlType is stepped.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="NumberOfSensors" type="xs:int" default="0" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Daylighting setpoint {lux}.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="MinimumInputPowerFraction" type="xs:double" default="0.3" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Minimum input power fraction.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="MinimumLightOutputFraction" type="xs:double" default="0.2" minOccurs="0">
                            <xs:annotation>
                              <xs:documentation>Minimum light output fraction.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Sensor_1" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="x" type="xs:double"/>
                                <xs:element name="y" type="xs:double"/>
                                <xs:element name="z" type="xs:double"/>
                                <xs:element name="FractionOfZone" type="xs:double">
                                  <xs:annotation>
                                    <xs:documentation>Fraction of zone controlled by sensor.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Sensor_2" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="x" type="xs:double"/>
                                <xs:element name="y" type="xs:double"/>
                                <xs:element name="z" type="xs:double"/>
                                <xs:element name="FractionOfZone" type="xs:double">
                                  <xs:annotation>
                                    <xs:documentation>Fraction of zone controlled by sensor.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attributeGroup ref="CostDist"/>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Infiltration" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Zone infiltration.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:choice>
                            <xs:element name="Rate" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Design Air Changes per Hour {ACH}.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="FlowPerExtWallArea" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Infiltration flow rate per exterior wall area {(m3/s)/m2}.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="FlowPerExtArea" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Infiltration flow rate per exterior area {(m3/s)/m2}.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:choice>
                          <xs:element ref="Schedule" minOccurs="0"/>
                        </xs:sequence>
                        <xs:attributeGroup ref="CostDist"/>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="DetachedShading" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CustomXYZSurfaces" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="SurfaceType">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:enumeration value="Fixed"/>
                                <xs:enumeration value="Building"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="SurfaceName" type="xs:string"/>
                          <xs:element name="ShadingTransmittanceSchedule" type="xs:string"/>
                          <xs:element name="Vertices" minOccurs="3" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:attribute name="X1" type="xs:double" default="1"/>
                              <xs:attribute name="Y1" type="xs:double" default="1"/>
                              <xs:attribute name="Z1" type="xs:double" default="0"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="PV-Simple" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Simple pv modelling.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Mode">
                      <xs:annotation>
                        <xs:documentation>Stringkey denoting which surfaces PV is installed.</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:enumeration value="Roof Only"/>
                          <xs:enumeration value="Roof and South Facade"/>
                          <xs:enumeration value="All Opaque Exterior"/>
                          <xs:enumeration value="Roof and Attached Shades"/>
                          <xs:enumeration value="South Facade Only"/>
                          <xs:enumeration value="North Facade Only"/>
                          <xs:enumeration value="Existing Surfaces"/>
