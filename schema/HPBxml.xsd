                              </xs:complexType>
                            </xs:element>
                            <xs:element name="OutdoorAirUnit" minOccurs="0">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="OutdoorAirFlowRate" type="xs:double" minOccurs="0">
