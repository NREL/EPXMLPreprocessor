                                  </xs:element>
                                  <xs:element name="LowAirTempSchedule" type="xs:string" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>User must input schedule as IDF text.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="AvailabilitySchedule" type="xs:string" minOccurs="0"/>
                                  <xs:element name="Equipment" minOccurs="0">
                                    <xs:complexType>
                                      <xs:sequence minOccurs="0">
                                        <xs:choice minOccurs="0" maxOccurs="unbounded">
                                          <xs:element name="Fan">
                                            <xs:complexType>
                                              <xs:sequence>
                                                <xs:element name="Type" type="FanType"/>
                                                <xs:element name="Kind">
                                                  <xs:simpleType>
                                                    <xs:restriction base="xs:string">
                                                      <xs:enumeration value="Supply"/>
                                                      <xs:enumeration value="Exhaust"/>
                                                    </xs:restriction>
                                                  </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="Efficiency" type="xs:double"/>
                                                <xs:element name="PressureDrop" type="xs:double">
                                                  <xs:annotation>
                                                    <xs:documentation>{Pa}</xs:documentation>
                                                  </xs:annotation>
                                                </xs:element>
                                                <xs:element ref="Schedule" minOccurs="0"/>
                                              </xs:sequence>
                                            </xs:complexType>
                                          </xs:element>
                                          <xs:element name="ERV" minOccurs="0">
                                            <xs:complexType>
                                              <xs:sequence>
                                                <xs:element name="SensibleEffectiveness" type="xs:double"/>
                                                <xs:element name="LatentEffectiveness" type="xs:double"/>
                                                <xs:element name="HeatExchangerType" minOccurs="0">
                                                  <xs:annotation>
                                                    <xs:documentation>Optional, defaults to Plate.</xs:documentation>
                                                  </xs:annotation>
                                                  <xs:simpleType>
                                                    <xs:restriction base="xs:string">
                                                      <xs:enumeration value="Plate"/>
                                                      <xs:enumeration value="Rotary"/>
                                                    </xs:restriction>
                                                  </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="FrostControlType" minOccurs="0">
                                                  <xs:annotation>
                                                    <xs:documentation>Optional, defauls to None.</xs:documentation>
                                                  </xs:annotation>
                                                  <xs:simpleType>
                                                    <xs:restriction base="xs:string">
                                                      <xs:enumeration value="None"/>
                                                      <xs:enumeration value="ExhaustAirRecirculation"/>
                                                      <xs:enumeration value="ExhaustOnly"/>
                                                      <xs:enumeration value="MinimumExhaustTemperature"/>
                                                    </xs:restriction>
                                                  </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="ThresholdTemp" type="xs:double" minOccurs="0">
                                                  <xs:annotation>
                                                    <xs:documentation>Optional, defaults to 1.7 {deg C}.</xs:documentation>
                                                  </xs:annotation>
                                                </xs:element>
                                                <xs:element name="InitialDefrostTime" type="xs:double" minOccurs="0">
                                                  <xs:annotation>
                                                    <xs:documentation>Optional, defaults to 0.083 {min/min}. </xs:documentation>
                                                  </xs:annotation>
                                                </xs:element>
                                                <xs:element name="RateDefrostTimeIncrease" type="xs:double" minOccurs="0">
                                                  <xs:annotation>
                                                    <xs:documentation>Optional, defaults to 0.012 {min/min per deg C}.</xs:documentation>
                                                  </xs:annotation>
                                                </xs:element>
                                              </xs:sequence>
                                            </xs:complexType>
                                          </xs:element>
                                          <xs:element name="Coil" minOccurs="0">
                                            <xs:complexType>
                                              <xs:sequence>
                                                <xs:element name="Type" type="CoilType"/>
                                                <xs:element name="Fuel">
                                                  <xs:simpleType>
                                                    <xs:restriction base="CoilFuel"/>
                                                  </xs:simpleType>
                                                </xs:element>
                                                <xs:element name="Efficiency" type="xs:double" minOccurs="0"/>
                                                <xs:element name="COP" type="xs:double" minOccurs="0"/>
                                              </xs:sequence>
                                            </xs:complexType>
                                          </xs:element>
                                        </xs:choice>
                                      </xs:sequence>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="ExhaustFan" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Exhaust Fan</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:choice>
                                    <xs:element name="ExhaustFlowPerArea" type="xs:double">
                                      <xs:annotation>
                                        <xs:documentation>exhaust flow in m3/s per m2.  </xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ExhaustFlow" type="xs:float">
                                      <xs:annotation>
                                        <xs:documentation>exhaust flow in m3/s</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ExhaustFlowACH">
                                      <xs:annotation>
                                        <xs:documentation>exhaust flow in ACH</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                  </xs:choice>
                                  <xs:element name="ExhaustFanEfficiency" type="xs:double"/>
                                  <xs:element name="ExhaustFanPressureDrop" type="xs:double"/>
                                  <xs:element name="MakeupInfiltrationFraction" type="xs:double" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>fraction of exhaust brought in by infiltration, [0.0..1.0]</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="MakeupTransferAir" minOccurs="0" maxOccurs="unbounded">
                                    <xs:annotation>
                                      <xs:documentation>Source of exhausted air is transfer from other zone</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element name="ZoneFrom" type="xs:string">
                                          <xs:annotation>
                                            <xs:documentation>name of zone supplying air</xs:documentation>
                                          </xs:annotation>
                                        </xs:element>
                                        <xs:choice>
                                          <xs:element name="FlowFraction" type="xs:double">
                                            <xs:annotation>
                                              <xs:documentation>fraction of total exhaust air from this zone</xs:documentation>
                                            </xs:annotation>
                                          </xs:element>
                                          <xs:element name="FlowRate" type="xs:float"/>
                                        </xs:choice>
                                      </xs:sequence>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element ref="Schedule" minOccurs="0"/>
                                  <xs:element name="OverrideOA" type="xs:boolean" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>If this element is selected 'Yes' then the OA flow to the zone can be overridden. The OA flow will be overriden by the preprocessor if the exhaust flow rate is larger than the OA flow rate to the zone. If this element is selected 'No' then the OA flow rate will not be affected. Defaults to 'Yes' in the preprocessor.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="ZoneERV" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Zone Energy Recovery Ventilator</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:all>
                                  <xs:element name="SupplyFanEfficiency" type="xs:double"/>
                                  <xs:element name="SupplyFanPressureDrop" type="xs:double"/>
                                  <xs:element name="ExhaustFanEfficiency" type="xs:double"/>
                                  <xs:element name="ExhaustFanPressureDrop" type="xs:double"/>
                                  <xs:element name="SensibleEffectiveness" type="xs:double"/>
                                  <xs:element name="LatentEffectiveness" type="xs:double"/>
                                  <xs:element name="ParasiticPower" type="xs:double"/>
                                  <xs:element name="UseEconomizer" type="xs:boolean"/>
                                </xs:all>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="PurchasedAir" minOccurs="0">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="OutdoorAir" type="xs:boolean" minOccurs="0">
                                    <xs:annotation>
                                      <xs:documentation>Defulats to true, which means their will be a fixed amount of oa mixed with the return air to make up the heating or cooling supply air stream.  The rate of outside air is based on the user's input under sone/sizing. </xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:sequence minOccurs="0">
