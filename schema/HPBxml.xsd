                                  <xs:enumeration value="Perimeter Corners and Core"/>
                                </xs:restriction>
                              </xs:simpleType>
                            </xs:element>
                            <xs:element name="ZoneMoniker" type="xs:string" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Optional.  More description name to use in EnergyPlus input files.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="SectionDescription" type="xs:string" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Description of section.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:all>
                          <xs:attribute name="Length1" type="xs:double" default="1.0">
                            <xs:annotation>
                              <xs:documentation>Length of building (X-Direction) {m}.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="Width1" type="xs:double" use="optional" default="1.0">
                            <xs:annotation>
                              <xs:documentation>Width of building (Y-Direction) {m}</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="Length2" type="xs:double" default="1.0">
                            <xs:annotation>
                              <xs:documentation>Length of courtyard (Y-Direction) {m}</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="Width2" type="xs:double" use="optional" default="1.0">
                            <xs:annotation>
                              <xs:documentation>Width of courtyard (Y-Direction) {m}</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                        </xs:complexType>
