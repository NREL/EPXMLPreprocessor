                      <xs:element name="L-Shape">
                        <xs:annotation>
                          <xs:documentation>L-Shape Buidling</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:all>
                            <xs:element name="ZoneLayout">
                              <xs:annotation>
                                <xs:documentation>Zone configuration for selected shape.</xs:documentation>
                              </xs:annotation>
                              <xs:simpleType>
                                <xs:restriction base="xs:string">
                                  <xs:enumeration value="Minimum Zones"/>
                                  <xs:enumeration value="Perimeter and Core"/>
                                  <xs:enumeration value="Perimeter Corners and Core"/>
                                </xs:restriction>
                              </xs:simpleType>
                            </xs:element>
                            <xs:element name="ZoneMoniker" type="xs:string" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Optional.  More description name to use in EnergyPlus input files.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="SectionDescription" type="xs:string" minOccurs="0">
                              <xs:annotation>
                                <xs:documentation>Description of section.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:all>
                          <xs:attribute name="Length1" type="xs:double" default="1.0">
                            <xs:annotation>
                              <xs:documentation>Length of building (X-Direction) {m}.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
