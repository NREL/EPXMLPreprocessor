      <xs:sequence>
        <xs:element name="ApplyType" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Fraction of exterior wall area that are windows.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="BottomUp"/>
              <xs:enumeration value="TopDown"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="AreaFraction" default="0.4" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Fraction of exterior wall area that are windows.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:double">
                <xs:attributeGroup ref="CostDist"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="SillHeight" default="1.1" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Height offset for windows {m}.  If area available is less than requested area, then the sill height will be reduced in an attempt to fit the glazing.  This only applies if ApplyType is bottomup</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:double">
                <xs:attributeGroup ref="CostDist"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="HeaderHeight" default="0.5" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Header height offset for windows {m}.  If area available is less than requested area, then the header height will be reduced in an attempt to fit the glazing. This only applies if ApplyType is topdown.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:double">
                <xs:attributeGroup ref="CostDist"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="EdgeOffset" default="0.05" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Offsets for the edges of the window {m}.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:double">
                <xs:attributeGroup ref="CostDist"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="Type" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Only Banded glazing works currently.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Banded"/>
              <xs:enumeration value="Discrete"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Construction" type="WindowConstruction" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Custom construction for this window.  Overrides the standard construction.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Overhang" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Window overhang.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:choice>
                <xs:element name="Depth">
                  <xs:annotation>
                    <xs:documentation>Depth of window overhang {m}.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:simpleContent>
                      <xs:extension base="xs:double">
                        <xs:attributeGroup ref="CostDist"/>
                      </xs:extension>
                    </xs:simpleContent>
                  </xs:complexType>
                </xs:element>
                <xs:element name="ProjectionFactor">
                  <xs:annotation>
                    <xs:documentation>Fraction of window height that is projected out as overhang.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:simpleContent>
                      <xs:extension base="xs:double">
                        <xs:attributeGroup ref="CostDist"/>
                      </xs:extension>
                    </xs:simpleContent>
                  </xs:complexType>
                </xs:element>
              </xs:choice>
              <xs:element name="Offset">
                <xs:annotation>
                  <xs:documentation>Offset above window where overhang starts {m}.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Fin" minOccurs="0">
          <xs:annotation>
            <xs:documentation>Window fins.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:choice>
                <xs:element name="Depth">
