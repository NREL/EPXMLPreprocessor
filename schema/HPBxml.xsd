                </xs:annotation>
              </xs:element>
              <xs:element name="LegalNoticeType" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>This will add a legal notice to the IDF file.  The default is the Example File Generator notice.  If a custom legal notice is needed, then set this to none and add the legal notice to the HeaderInformation.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value=""/>
                    <xs:enumeration value="None"/>
                    <xs:enumeration value="EEFG"/>
                    <xs:enumeration value="Benchmarks"/>
                    <xs:enumeration value="General"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Area">
                <xs:annotation>
                  <xs:documentation>Only applies when using non-custom formats</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="NumFloors">
                <xs:annotation>
                  <xs:documentation>Only applies when Footprint is set to true.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="AtticHeight" type="xs:double" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>only valid to add a flat attic to the building {m}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PerimDepth">
                <xs:annotation>
                  <xs:documentation>Depth of perimeter zones {m}.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:double">
                      <xs:attributeGroup ref="CostDist"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="FloortoFloorHeight" type="xs:double" default="3.8">
                <xs:annotation>
                  <xs:documentation>Floor to floor height of space {m}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="PlenumHeight" type="xs:double" default="0.9" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Hieght of plenum {m}. If set to 0, then will model as if no plenum.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Rotation" type="xs:double">
                <xs:annotation>
                  <xs:documentation>Rotation of building from North clockwise {deg}.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Terrain" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Type of building terrain.  Defaults to City.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="City"/>
                    <xs:enumeration value="Suburbs"/>
                    <xs:enumeration value="Country"/>
                    <xs:enumeration value="Ocean"/>
                    <xs:enumeration value="Urban"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="SolarDistribution" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:enumeration value="MinimalShadowing"/>
                    <xs:enumeration value="FullExterior"/>
                    <xs:enumeration value="FullInteriorAndExterior"/>
                    <xs:enumeration value="FullExteriorWithReflections"/>
                    <xs:enumeration value="FullInteriorAndExteriorWithReflections"/>
                  </xs:restriction>
