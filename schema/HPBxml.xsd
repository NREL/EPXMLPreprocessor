                      </xs:annotation>
                    </xs:element>
                    <xs:element name="east" type="WindowConstruction" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Windows on east facade</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="north" type="WindowConstruction" minOccurs="0">
                      <xs:annotation>
