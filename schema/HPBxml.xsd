        <xs:element name="Building">
          <xs:annotation>
            <xs:documentation>This is the basic building information.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Name" type="xs:string" minOccurs="0">
                <xs:annotation>
