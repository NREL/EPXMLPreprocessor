#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = EPXMLPreproc2.exe EPXMLPreproc2Tests.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

EPXMLPreproc2.exe: EPXMLPreproc2.dpr
  $(DCC)

EPXMLPreproc2Tests.exe: dunit\EPXMLPreproc2Tests.dpr
  $(DCC)


